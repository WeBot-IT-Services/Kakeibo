# Production Environment Variables for Kakeibo

# Copy this file to .env.production.local for production deployment
# DO NOT commit this file with actual values to version control

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# AI Services
NEXT_PUBLIC_OCR_API_KEY=your_ocr_api_key
NEXT_PUBLIC_OPENAI_API_KEY=your_openai_api_key

# Authentication
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=https://your-app-name.vercel.app

# Environment
NODE_ENV=production